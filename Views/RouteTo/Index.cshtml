@model IEnumerable< MyEticketApplication.Models.RouteTo>
<h1>Route To Information</h1>
<div>
    <a class="btn btn-primary" asp-action="Create" asp-controller="RouteTo">Create</a>
    <table class="table">
        <thead>
            <tr>
                <th>
                    Name
                </th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var item in Model)
            {
                <tr>
                    <td>@item.RouteToName</td>
                    <td>
                        <a class="btn btn-secondary" asp-action="Edit" asp-controller="RouteTo" asp-route-RouteToId="@item.RouteToId">Edit</a>
                        <a asp-action="Delete" class="btn btn-danger" asp-controller="RouteTo" asp-action="Delete" asp-route-RouteToId="@item.RouteToId">Delete</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>